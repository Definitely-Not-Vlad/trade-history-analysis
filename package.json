{
  "name": "trade-history-analysis",
  "version": "1.0.0",
  "description": "Analyze your Binance trade history",
  "main": "src/main.js",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "dist": "electron-builder",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "start": "react-scripts build && electron .",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "~5.11.4",
    "@testing-library/react": "~11.1.0",
    "@testing-library/user-event": "~12.1.10",
    "auto-bind": "~4.0.0",
    "crypto-js": "~4.0.0",
    "electron-fetch": "~1.7.3",
    "prop-types": "~15.7.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-redux": "~7.2.4",
    "react-scripts": "4.0.3",
    "redux": "~4.1.0",
    "redux-logger": "~3.0.6",
    "redux-persist": "~6.0.0",
    "redux-thunk": "~2.3.0",
    "urijs": "~1.19.6",
    "web-vitals": "~1.0.1"
  },
  "devDependencies": {
    "electron": "~13.0.1",
    "electron-builder": "~22.10.5",
    "eslint-plugin-prettier": "~3.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.definitelynotvlad.tradehistoryanalysis",
    "directories": {
      "files": [
        "assets/icon.png"
      ],
      "output": "dist-build"
    },
    "mac": {
      "category": "public.app-category.finance",
      "extraDistFiles": [
        "assets/icon.png"
      ],
      "hardenedRuntime": false,
      "target": "pkg"
    },
    "nsis": {},
    "pkg": {
      "isVersionChecked": false
    },
    "productName": "Trade History Analysis",
    "win": {
      "target": "portable",
      "signAndEditExecutable": false
    }
  }
}
